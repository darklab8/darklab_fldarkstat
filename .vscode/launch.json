{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            // "port": 2345,
            "host": "127.0.0.1",
            "program": "${workspaceFolder}",
            "args": [
                "configs",
            ],
            "env": {
                "ENVERANT_ENV_FILE": "${workspaceFolder}/.vscode/enverant.json",
                "GRPC_TRACE": "all",
                "GRPC_VERBOSITY": "DEBUG",
                "GRPC_GO_LOG_SEVERITY_LEVEL": "info",
                "GRPC_GO_LOG_VERBOSITY_LEVEL": "6",
                "GODEBUG": "http2debug=1",
            },
            "showLog": true
        }
    ]
}